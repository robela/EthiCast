$(function(){var n=$("iframe[src^='http://player.vimeo.com'], iframe[src^='http://www.youtube.com'], object, embed"),t=$("figure");n.each(function(){$(this).attr("data-aspectRatio",this.height/this.width).removeAttr("height").removeAttr("width")});$(window).resize(function(){var i=t.width();n.each(function(){var n=$(this);n.width(i).height(i*n.attr("data-aspectRatio"))})}).resize()})